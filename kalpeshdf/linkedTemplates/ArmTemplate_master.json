{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kalpeshdf"
		},
		"kalpeshblobstorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'kalpeshblobstorage'"
		},
		"KalpeshSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'KalpeshSQLServer'"
		},
		"kalpeshcosmosdb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'kalpeshcosmosdb'"
		},
		"AzureKey_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azure-secret.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kalpeshdf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"kalpeshblobstorage_connectionString": {
						"value": "[parameters('kalpeshblobstorage_connectionString')]"
					},
					"KalpeshSQLServer_connectionString": {
						"value": "[parameters('KalpeshSQLServer_connectionString')]"
					},
					"kalpeshcosmosdb_connectionString": {
						"value": "[parameters('kalpeshcosmosdb_connectionString')]"
					},
					"AzureKey_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKey_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}