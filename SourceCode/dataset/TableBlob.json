{
	"name": "TableBlob",
	"properties": {
		"linkedServiceName": {
			"referenceName": "kalpeshblobstorage",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"tablename": {
				"type": "string",
				"defaultValue": "@item.tablename"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().tablename",
					"type": "Expression"
				},
				"folderPath": "TableData",
				"container": "adf-container"
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"quoteChar": "\""
		},
		"schema": []
	}
}